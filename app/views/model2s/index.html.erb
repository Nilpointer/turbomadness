<%= javascript_pack_tag 'model2', 'data-turbolinks-track': 'reload' %>
<p id="notice"><%= notice %></p>

<h1>Model2s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @model2s.each do |model2| %>
      <tr>
        <td><%= model2.title %></td>
        <td><%= link_to 'Show', model2 %></td>
        <td><%= link_to 'Edit', edit_model2_path(model2) %></td>
        <td><%= link_to 'Destroy', model2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Model1', model1s_path %>
