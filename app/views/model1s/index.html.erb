<%= javascript_pack_tag 'model1', 'data-turbolinks-track': 'reload' %>
<p id="notice"><%= notice %></p>

<h1>Model1s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @model1s.each do |model1| %>
      <tr>
        <td><%= model1.title %></td>
        <td><%= link_to 'Show', model1 %></td>
        <td><%= link_to 'Edit', edit_model1_path(model1) %></td>
        <td><%= link_to 'Destroy', model1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

